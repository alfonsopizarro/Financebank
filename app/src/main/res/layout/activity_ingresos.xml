<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fotoingreso">

    <EditText
        android:id="@+id/nombreingresos"
        android:layout_width="183dp"
        android:layout_height="48dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="128dp"
        android:layout_marginBottom="440dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#050404"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/cantidadingresos"
        android:layout_width="174dp"
        android:layout_height="48dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="337dp"
        android:layout_marginEnd="137dp"
        android:layout_marginBottom="276dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#050404"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/nombrei"
        android:layout_width="163dp"
        android:layout_height="21dp"
        android:layout_marginStart="103dp"
        android:layout_marginTop="159dp"
        android:layout_marginEnd="145dp"
        android:layout_marginBottom="479dp"
        android:text="NOMBRE DEL INGRESO"
        android:textColor="#050404"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fechanombre"
        android:layout_width="160dp"
        android:layout_height="18dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="237dp"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="404dp"
        android:text="FECHA"
        android:textColor="#050404"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/cantidadnombre"
        android:layout_width="161dp"
        android:layout_height="19dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="316dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="324dp"
        android:text="CANTIDAD EUROS"
        android:textColor="#050404"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonaceptaringresos"
        android:layout_width="135dp"
        android:layout_height="36dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="533dp"
        android:backgroundTint="#0B4C4A"
        android:text="Aceptar"
        android:textColor="#F3EEEE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/botonvolveringresos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="482dp"
        android:layout_marginEnd="223dp"
        android:layout_marginBottom="129dp"
        android:backgroundTint="#9E1D1D"
        android:text="volver"
        android:textColor="#F3EEEE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonsaliringresos"
        android:layout_width="85dp"
        android:layout_height="37dp"
        android:layout_marginStart="323dp"
        android:layout_marginTop="615dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="7dp"
        android:backgroundTint="#9E1D1D"
        android:text="Salir"
        android:textColor="#F3EEEE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/fechaingresos"
        android:layout_width="183dp"
        android:layout_height="48dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="255dp"
        android:layout_marginEnd="128dp"
        android:layout_marginBottom="356dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>