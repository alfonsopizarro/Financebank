<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/hoja1">

    <Button
        android:id="@+id/botoningresos"
        android:layout_width="400dp"
        android:layout_height="55dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="301dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="303dp"
        android:backgroundTint="#0B4C4A"
        android:text="Ingresos"
        android:textColor="#F3EEEE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botongastos"
        android:layout_width="400dp"
        android:layout_height="55dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="369dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="235dp"
        android:backgroundTint="#0B4C4A"
        android:text="Gastos"
        android:textColor="#F3EEEE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonhistorial"
        android:layout_width="400dp"
        android:layout_height="55dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="434dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="170dp"
        android:backgroundTint="#0B4C4A"
        android:text="Historial"
        android:textColor="#F3EEEE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonsalirpantallaprincipal"
        android:layout_width="85dp"
        android:layout_height="37dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="553dp"
        android:layout_marginEnd="244dp"
        android:layout_marginBottom="69dp"
        android:backgroundTint="#9E1D1D"
        android:text="Salir"
        android:textColor="#F3EEEE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/botongraficos"
        android:layout_width="400dp"
        android:layout_height="55dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="372dp"
        android:backgroundTint="#0B4C4A"
        android:text="Graficos"
        android:textColor="#F3EEEE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/dineroactual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="169dp"
        android:layout_marginBottom="532dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="151dp"
        android:layout_height="25dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="228dp"
        android:layout_marginBottom="577dp"
        android:text="Tu dinero"
        android:textColor="#0B0101"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>